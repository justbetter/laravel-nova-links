{
    "name": "justbetter/laravel-nova-links",
    "description": "Easily add a links section to your Laravel Nova application",
    "type": "package",
    "license": "MIT",
    "homepage": "https://github.com/justbetter/laravel-nova-links",
    "authors": [
        {
            "name": "Ramon Rietdijk",
            "email": "ramon@justbetter.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "laravel/nova": "^4.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.6",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "JustBetter\\NovaLinks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustBetter\\NovaLinks\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan",
        "style": "pint --test",
        "quality": [
            "@test",
            "@analyse",
            "@style"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
